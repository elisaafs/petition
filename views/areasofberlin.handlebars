<div class="center-wrapper">
    <div class="container2">
        <div class="title">{{length}} signer(s) in {{area_of_berlin}}</div>
        <div>{{error}}</div>
        <div class="scrollable">
        {{#content}}
            {{#if homepage}}
                <a href="{{homepage}}">{{first_name}} {{last_name}},</a>
             {{else}}
                {{first_name}} {{last_name}},
            {{/if}}
            {{#if age}}
               {{age}}
            {{/if}}
        {{/content}}
        </div>
        <div class="buttons-signerspage">
            <input class="button-signers" id="goback" type="button" name="button" value="GO BACK"></input>
        </div>
    </div>
</div>

<input class="button-logout" id="button-logout" type="button" name="button" value="LOGOUT"></input>

<img class="logo" src="/bvg.png">
<script src="/script2.js" type="text/javascript" charset="utf-8"></script>
